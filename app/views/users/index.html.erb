<!--ユーザー一覧画面-->
<h1>User info</h1>
 <p>image</p>
  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %>
  <p>name <%= @user.name %></p>
  <p>introduction <%= @user.introduction %></p>



<h1>Users</h1>
<% @users.each do |user| %>
  <p>image</p>
  <%= attachment_image_tag user, :profile_image, :fill, 30, 30, fallback: "no_image.jpg" %>
  <p>name</p>
  <%= user.name %>
<% end %>


